{
    "name": "gymBuddy",
    "version": "6.0.0",
    "license": "Apache",
    "config": {
        "s3BucketName": "",
        "region": "ap-southeast-1"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve --ec true",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "list-s3": "aws s3 ls",
        "s3-upload": "aws s3 cp ./dist s3://%npm_package_config_s3BucketName% --recursive --acl public-read --region %npm_package_config_region%",
        "s3-build-upload-qa": "ng build && aws s3 rm s3://%npm_package_config_s3BucketName% --recursive && aws s3 cp ./dist s3://%npm_package_config_s3BucketName% --recursive --acl public-read --region %npm_package_config_region%",
        "s3-build-upload-prod": "ng build --prod && aws s3 rm s3://%npm_package_config_s3BucketName% --recursive && aws s3 cp ./dist s3://%npm_package_config_s3BucketName% --recursive --acl public-read --region %npm_package_config_region%"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^6.0.0",
        "@angular/cdk": "^6.2.1",
        "@angular/common": "^6.0.0",
        "@angular/compiler": "^6.0.0",
        "@angular/core": "^6.0.0",
        "@angular/forms": "^6.0.0",
        "@angular/http": "^6.0.0",
        "@angular/material": "^6.2.1",
        "@angular/platform-browser": "^6.0.0",
        "@angular/platform-browser-dynamic": "^6.0.0",
        "@angular/router": "^6.0.0",
        "@ng-bootstrap/ng-bootstrap": "^2.0.0",
        "@ngx-translate/core": "^10.0.1",
        "@ngx-translate/http-loader": "^3.0.1",
        "bootstrap": "^4.1.1",
        "chart.js": "^2.7.2",
        "core-js": "^2.5.5",
        "font-awesome": "^4.7.0",
        "hammerjs": "^2.0.8",
        "jquery": "^3.3.1",
        "ng2-charts": "^1.6.0",
        "rxjs": "^6.1.0",
        "rxjs-compat": "^6.2.1",
        "web-animations-js": "^2.3.1",
        "yarn": "^1.6.0",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.0",
        "@angular/cli": "6.0.0",
        "@angular/compiler-cli": "^6.0.0",
        "@angular/language-service": "^6.0.0",
        "@types/datatables.net": "^1.10.12",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/jquery": "^3.3.5",
        "@types/node": "^9.6.1",
        "codelyzer": "~4.2.1",
        "jasmine-core": "~3.1.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^1.0.0",
        "protractor": "~5.3.0",
        "ts-node": "~5.0.1",
        "tslint": "~5.9.1",
        "typescript": "~2.7.0"
    }
}
